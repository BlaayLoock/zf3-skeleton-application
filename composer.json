{
  "name": "jiromm/zf3-skeleton-application",
  "description": "Skeleton Application for Zend Framework applications",
  "type": "project",
  "license": "BSD-3-Clause",
  "keywords": [
    "framework",
    "mvc",
    "zf2"
  ],
  "homepage": "https://github.com/jiromm/zf3-skeleton-application",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-component-installer": "^1.0 || ^0.3 || ^1.0.0-dev@dev",
    "zendframework/zend-mvc": "^3.0.1",
    "zendframework/zend-db": "^2.8.1",
    "zendframework/zend-mvc-form": "^1.0",
    "zendframework/zend-mvc-console": "^1.1.10",
    "zendframework/zend-servicemanager-di": "^1.0",
    "zendframework/zend-hydrator": "^2.2",
    "zendframework/zend-json": "^3.0",
    "zendframework/zend-i18n": "^2.7"
  },
  "require-dev": {
    "zendframework/zend-test": "^3.0",
    "zendframework/zend-developer-tools": "^1.0.1",
    "zfcampus/zf-development-mode": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Settings\\": "module/Settings/src/",
      "Demo\\": "module/Demo/src/"
    }
  },
  "scripts": {
    "development-disable": "zf-development-mode disable",
    "development-enable": "zf-development-mode enable",
    "development-status": "zf-development-mode status",
    "serve": "php -S 0.0.0.0:8080 -t public/ public/index.php"
  }
}
